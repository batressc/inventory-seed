{
  "name": "batriano-app",
  "version": "0.0.1",
  "scripts": {
    "clean": "del dist\\*.* /s /q",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build": "npm run clean && tsc",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "browserify": "browserify -s main dist/main.js > dist/bundle.js",
    "rollup": "rollup -f iife -c -o dist/bundle.es2015.js",
    "es5": "tsc --target es5 --allowJs dist/bundle.es2015.js --out dist/bundle.js",
    "build_semiprod": "npm run build && npm run browserify && npm run minify",
    "build_prod": "npm run build && npm run rollup && npm run es5 && npm run minify"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "rxjs-es": "^5.0.0-beta.10",
    "semantic-ui": "^2.2.2",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "rollup": "^0.33.0",
    "rollup-plugin-node-resolve": "^1.7.1",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "uglifyjs": "^2.4.10"
  }
}
